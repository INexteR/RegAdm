<local:ReservationDialogBase x:Class="RegAdm.Dialogs.ReservationDialog"
                             x:ClassModifier="internal"
                             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                             xmlns:local="clr-namespace:RegAdm.Dialogs"
                             mc:Ignorable="d"
                             Height="450"
                             Width="800"
                             WindowStartupLocation="CenterScreen"
                             Style="{StaticResource ctrlStyle}">
    <Window.Resources>
        <local:ReservationDialogData x:Key="dialogData" />
        <Style TargetType="TextBlock"
               BasedOn="{StaticResource labelText}" />
    </Window.Resources>
    <Grid>
        
    </Grid>
</local:ReservationDialogBase>
